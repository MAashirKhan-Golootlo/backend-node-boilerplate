{
  "name": "golootlo-backend-starter",
  "version": "1.0.0",
  "description": "Feature-based Express + TypeScript backend starter",
  "main": "dist/core/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/core/server.ts",
    "build": "rimraf dist && tsc --project tsconfig.json",
    "start": "node dist/core/server.js",
    "lint": "eslint \"src/**/*.ts\"",
    "folderlint": "node scripts/folderlint.js"
  },
  "keywords": [
    "express",
    "typescript",
    "starter",
    "feature-based"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "pino": "^9.3.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.6",
    "@types/swagger-jsdoc": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.14.0",
    "rimraf": "^5.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.18.0"
  }
}
